files:
    "/home/ec2-user/.bashrc":
        mode: "000777"
        owner: ec2-user
        group: ec2-user
        content: |
            # .bashrc

            # Source global definitions
            if [ -f /etc/bashrc ]; then
                    . /etc/bashrc
            fi

            cd /var/app/current
            echo "Automatically navigated to /var/app/current"
